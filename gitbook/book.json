{
  "title": "Economic Coercion and Strategic Competition",
  "description": "A comprehensive textbook on economic statecraft in US-China relations",
  "author": "Laurence Wilse-Samson",
  "language": "en",
  "gitbook": "3.2.3",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "styles": {
    "website": "styles.css"
  },
  "plugins": [
    "expandable-chapters",
    "back-to-top-button",
    "copy-code-button",
    "-sharing",
    "anchors",
    "fontsettings",
    "theme-default"
  ],
  "pluginsConfig": {
    "fontsettings": {
      "theme": "white",
      "family": "serif",
      "size": 2
    },
    "theme-default": {
      "showLevel": true
    }
  },
  "pdf": {
    "pageNumbers": true,
    "fontSize": 12,
    "fontFamily": "Georgia",
    "paperSize": "letter",
    "margin": {
      "right": 62,
      "left": 62,
      "top": 56,
      "bottom": 56
    }
  },
  "links": {
    "sidebar": {
      "Author Website": "https://wagner.nyu.edu"
    }
  }
}
